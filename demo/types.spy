cfunc print(n: int) -> void;

Vector2: struct {
  x: int;
  y: int;
};

func factorial(n: int) -> int {
  if (n <= 1) {
    return 1;
  }
  return n * factorial(n - 1);
}

func fib(n: int) -> int {
  if (n <= 1) {
    return 1;
  }
  return fib(n - 1) + fib(n - 2);
}

func main() -> void {
  
  v: Vector2;
  v = new Vector2;
  v.x = 5;
  v.y = 6;

  print(factorial(v.x));
  print(fib(v.y));

}
